<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">
  <display-name>CloudGYM</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>CoachMenuServlet</servlet-name>
  	<servlet-class>com.coachMenu.controller.CoachMenuServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CoachMenuServlet</servlet-name>
  	<url-pattern>/coachMenu/coachMenu.do</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>OrdersServlet</servlet-name>
  	<servlet-class>com.orders.controller.OrdersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>OrdersServlet</servlet-name>
  	<url-pattern>/orders/orders.do</url-pattern>
  </servlet-mapping>
</web-app>