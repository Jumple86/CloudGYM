<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>CloudGYM</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>CoachMenuServlet</servlet-name>
		<servlet-class>com.coachMenu.controller.CoachMenuServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CoachMenuServlet</servlet-name>
		<url-pattern>/coachMenu/coachMenu.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>OrdersServlet</servlet-name>
		<servlet-class>com.orders.controller.OrdersServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OrdersServlet</servlet-name>
		<url-pattern>/orders/orders.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AdminServlet</servlet-name>
		<servlet-class>com.admin.controller.AdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AdminServlet</servlet-name>
		<url-pattern>/html/admin.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>UserAuthServlet</servlet-name>
		<servlet-class>com.userAuth.controller.UserAuthServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserAuthServlet</servlet-name>
		<url-pattern>/html/UserAuth.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CoachImageOutput</servlet-name>
		<servlet-class>com.coach.image.CoachImageOutput</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CoachImageOutput</servlet-name>
		<url-pattern>/html/CoachImageOutput</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>PostsImageOutput</servlet-name>
		<servlet-class>com.posts.image.PostsImageOutput</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PostsImageOutput</servlet-name>
		<url-pattern>/html/PostsImageOutput</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>VideoOutput</servlet-name>
		<servlet-class>com.video.video.VideoOutput</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>VideoOutput</servlet-name>
		<url-pattern>/html/VideoOutput</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>com.user.controller.UserServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/html/user.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>CoachImgServlet</servlet-name>
		<servlet-class>com.coach.controller.CoachImgServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CoachImgServlet</servlet-name>
		<url-pattern>/coachImg/coachImg.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>CoachServlet</servlet-name>
		<servlet-class>com.coach.controller.CoachServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>CoachServlet</servlet-name>
		<url-pattern>/coach/coach.do</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>SubListServlet</servlet-name>
		<servlet-class>com.subList.controller.SubListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SubListServlet</servlet-name>
		<url-pattern>/html/sublist.do</url-pattern>
	</servlet-mapping>

	<resource-ref>
		<description>DB Connection</description>
		<res-ref-name>jdbc/TestDB2</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>


</web-app>